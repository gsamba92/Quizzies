<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="300.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.TestTaker.TestViewController">
   <children>
      <Button fx:id="submit" disable="true" layoutX="501.0" layoutY="16.0" mnemonicParsing="false" onAction="#submitClick" text="Submit" />
      <Label fx:id="topicName" layoutX="268.0" layoutY="23.0" text="Topic Name" />
      <Label fx:id="question" layoutX="110.0" layoutY="87.0" />
      <RadioButton fx:id="option1" disable="true" layoutX="110.0" layoutY="133.0" mnemonicParsing="false">
         <toggleGroup>
            <ToggleGroup fx:id="option" />
         </toggleGroup></RadioButton>
      <RadioButton fx:id="option2" disable="true" layoutX="332.0" layoutY="133.0" mnemonicParsing="false" toggleGroup="$option" />
      <RadioButton fx:id="option3" disable="true" layoutX="110.0" layoutY="175.0" mnemonicParsing="false" toggleGroup="$option" />
      <RadioButton fx:id="option4" disable="true" layoutX="332.0" layoutY="175.0" mnemonicParsing="false" toggleGroup="$option" />
      <Button fx:id="next" disable="true" layoutX="378.0" layoutY="239.0" mnemonicParsing="false" onAction="#nextClick" text="Next" />
      <Separator layoutY="66.0" prefWidth="600.0" />
      <Button fx:id="beginTest" layoutX="58.0" layoutY="19.0" mnemonicParsing="false" onAction="#beginTestClick" text="Begin" />
   </children>
</AnchorPane>
